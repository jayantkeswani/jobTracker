<%- include layouts/header.ejs %>

    <div class="page-data">
        <div class="data-btn">
            <button onClick="addUser();">+ Add</button>
        </div>
        <div class="data-table">
            <table border="1" cellpadding="7" cellspacing="7">
                <tr>
                    <th width="50px">No</th>
                    <th>Company Name</th>
                    <th>Job Title</th>
                    <th>Description</th>
                    <th>Date Applied</th>
                </tr>

                <% if(data.length){

                 for(var i = 0;i < data.length;i++) { %>

                    <tr>
                        <td>
                            <%=(i+1)%>
                        </td>
                        <td>
                            <%=data[i].CompanyName%>
                        </td>
                        <td>
                            <%=data[i].JobTitle%>
                        </td>
                        <td>
                            <%=data[i].Description%>
                        </td>
                        <td>
                            <%=data[i].DateApplied%>
                        </td>
                    </tr>
                    <% }

             }else{ %>
                        <tr>
                            <td colspan="3">No Jobs</td>
                        </tr>
                        <% } %>



            </table>
        </div>
    </div>

    <%- include layouts/footer.ejs %>
